<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Memes</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>/Defs/TraitDef[defName="AM_Ahimsa"]</xpath>
			<value>
				<li Class="HautsFramework.ExciseTraitExemption" />
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Crystalloid</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>/Defs/TraitDef[defName="Attunement"]</xpath>
			<value>
				<li Class="HautsFramework.ExciseTraitExemption" />
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Reunion</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>/Defs/TraitDef[defName="ReunionCharacter"]</xpath>
			<value>
				<li Class="HautsFramework.ExciseTraitExemption" />
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Pirates</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraitDef[defName="VFEP_WarcasketTrait"]</xpath>
					<value>
						<li Class="HautsFramework.ExciseTraitExemption" />
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraitDef[defName="VFEP_Shellcasket"]</xpath>
					<value>
						<li Class="HautsFramework.ExciseTraitExemption" />
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Ideology Expanded - Memes and Structures</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>/Defs/TraitDef[defName="VME_Elder"]</xpath>
			<value>
				<li Class="HautsFramework.ExciseTraitExemption" />
			</value>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Psycasts Expanded</li>
		</mods>
		<match Class="PatchOperationAddModExtension">
			<xpath>/Defs/TraitDef[defName="VPE_Thrall"]</xpath>
			<value>
				<li Class="HautsFramework.ExciseTraitExemption" />
			</value>
		</match>
	</Operation>
</Patch>