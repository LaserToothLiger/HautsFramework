<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Recycling Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="VRecyclingE_Recycle_WastepackIntoChempack"]/workSpeedStat</xpath>
					<value>
						<workSpeedStat>Hauts_RecyclingSpeed</workSpeedStat>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="VRecyclingE_Recycle_ChempackIntoChemfuel"]/workSpeedStat</xpath>
					<value>
						<workSpeedStat>Hauts_RecyclingSpeed</workSpeedStat>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="VRecyclingE_Recycle_WastepackIntoToxpack"]/workSpeedStat</xpath>
					<value>
						<workSpeedStat>Hauts_RecyclingSpeed</workSpeedStat>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="VRecyclingE_Recycle_BreakDownToxpack"]/workSpeedStat</xpath>
					<value>
						<workSpeedStat>Hauts_RecyclingSpeed</workSpeedStat>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName="VRecyclingE_Recycle_IsotopackIntoUranium"]/workSpeedStat</xpath>
					<value>
						<workSpeedStat>Hauts_RecyclingSpeed</workSpeedStat>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>